                        {"type": "command", "name": "Shift spectrogram selection start 1 frame ahead", "command": "audio-shift-sel -s=+1f"},
                        {"type": "command", "name": "Shift spectrogram selection end 1 frame back", "command": "audio-shift-sel -e=-1f"},
                        {"type": "command", "name": "Shift spectrogram selection end 1 frame ahead", "command": "audio-shift-sel -e=+1f"},
                        {"type": "command", "name": "Shift selected subtitles start 1 second back", "command": "sub-set -s=-1000ms"},
                        {"type": "command", "name": "Shift selected subtitles start 1 second ahead", "command": "sub-set -s=+1000ms"},
                        {"type": "command", "name": "Shift selected subtitles end 1 second back", "command": "sub-set -e=-1000ms"},
                        {"type": "command", "name": "Shift selected subtitles end 1 second ahead", "command": "sub-set -e=+1000ms"}
